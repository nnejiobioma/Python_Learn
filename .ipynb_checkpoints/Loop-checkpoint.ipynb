      "Bye\n"
     ]
    }
   ],
   "source": [
    "# 5,9,12,14,15\n",
    "\n",
    "sum = 0\n",
    "n = int(input(\"N : \"))\n",
    "while n > 0:\n",
    "    sum = sum + n\n",
    "    n -= 1\n",
    "    print(\"Sum : \",sum)\n",
    "print(\"Bye\")"
   ]
